{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${{ secrets.DEFAULTCONNECTION }}"
  },
  "JwtSettings": {
    "SecretKey": "${{ secrets.JWTSECRETKEY }}",
    "Issuer": "microsoft",
    "Audience": "medfast",
    "AccessTokenExpiration": 30
  }
}

